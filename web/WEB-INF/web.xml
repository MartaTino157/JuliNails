<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <session-config>
        <session-timeout>
            3600
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <jsp-config>
        <jsp-property-group>
            <display-name>template1</display-name>
            <url-pattern>/WEB-INF/price.jsp</url-pattern>
            <url-pattern>/index.jsp</url-pattern>
            <page-encoding>UTF-8</page-encoding>
            <include-prelude>/WEB-INF/jspf/header.jspf</include-prelude>
            <include-coda>/WEB-INF/jspf/footer.jspf</include-coda>
        </jsp-property-group>
        <jsp-property-group>
            <display-name>template2</display-name>
            <url-pattern>/WEB-INF/adminManPage.jsp</url-pattern>
            <url-pattern>/WEB-INF/adminDepPage.jsp</url-pattern>
            <url-pattern>/WEB-INF/adminPanel.jsp</url-pattern>
            <url-pattern>/WEB-INF/adminPedForm.jsp</url-pattern>
            <url-pattern>/WEB-INF/adminManForm.jsp</url-pattern>
            <url-pattern>/WEB-INF/adminDepForm.jsp</url-pattern>
            <url-pattern>/WEB-INF/adminPedPage.jsp</url-pattern>
            <page-encoding>UTF-8</page-encoding>
            <include-prelude>/WEB-INF/jspf/adminHeader.jspf</include-prelude>
            <include-coda>/WEB-INF/jspf/adminFooter.jspf</include-coda>
        </jsp-property-group>
    </jsp-config>
</web-app>
